<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Excess_Quantity</fullName>
    <active>true</active>
    <errorConditionFormula>Quantity__c  &gt;  Stock_Name__r.Quantity__c</errorConditionFormula>
